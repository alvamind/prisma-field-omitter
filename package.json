{
  "name": "prisma-field-omitter",
  "version": "1.0.0",
  "description": "A tool to automatically hide or remove fields from Prisma-generated TypeScript types",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "prisma-field-omitter": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun build ./src/index.ts ./src/cli.ts --outdir ./dist --target node --minify",
    "prepublishOnly": "bun run clean && bun run build",
    "test": "bun test",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "prisma",
    "typescript",
    "types",
    "transformer",
    "cli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/prisma-field-omitter.git"
  },
  "dependencies": {
    "alvamind": "^1.0.10",
    "alvamind-tools": "^1.0.23",
    "ts-morph": "^25.0.1",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}